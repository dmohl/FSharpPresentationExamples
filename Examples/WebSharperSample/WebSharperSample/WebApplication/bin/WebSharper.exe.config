<?xml version="1.0" encoding="utf-8"?>
<!-- The configuration settings for can be defined here or overridden from the
     command-line. The command-line format is ~~KEY=VALUE where ~~ is a
     double-dash. -->
<configuration>
  <appSettings>
    <!-- The prefix of compiled code. -->
    <add key="Prefix" value="W$" />
    <!-- Whether the command-line compiler should display the greeting. -->
    <add key="Greet" value="true" />
    <!-- Trace level. Possible values are Off, Verbose, Info, Warning, Error. -->
    <add key="Trace" value="Info" />
    <!-- Path to put JavaScript files to. -->
    <add key="OutputPath" value="." />
    <!-- IntelliFactory.WebSharper.Plugins.ILocator implementation. -->
    <add key="Locator" value="IntelliFactory.WebSharper.Cci.Locator, IntelliFactory.WebSharper.Cci" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>